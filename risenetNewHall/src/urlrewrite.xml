<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
 	<rule>
        <from>^/([a-zA-Z]+)/([a-zA-Z]+).html$</from>
        <to type="forward">/$1/$2.YS</to>
    </rule>
    
    <rule>
        <from>^/([a-zA-Z]+)/([a-zA-Z]+)/([a-zA-Z]+)/([\{A-Z0-9\-\}]+).html$</from>
        <to type="forward">/$1/$2.YS?$3=$4</to>
    </rule>
    <rule>
        <from>^/([a-zA-Z]+)/([a-zA-Z]+)/([a-zA-Z]+)/([\{A-Z0-9\-\}]+)/([a-zA-Z]+)/([\{A-Z0-9\-\}]+)/action.html$</from>
        <to type="forward">/$1/$2.YS?$3=$4&amp;$5=$6</to>
    </rule>
    <rule>
        <from>^/([a-z]+)/([a-zA-Z]+)/([a-zA-Z]+)/([a-zA-Z]+)/([a-zA-Z]+)/([\{A-Z0-9\-\}]+).html$</from>
        <to type="forward">/$1/$2?$3=$4&amp;$5=$6</to>
    </rule>
    <rule>
        <from>^/([a-zA-Z]+)/([a-zA-Z]+)/([a-zA-Z]+)/([\{A-Z0-9\-\}]+)/([a-zA-Z]+)/([\{A-Z0-9\-\}]+)/([a-zA-Z]+)/([\{A-Z0-9\-\}]+).html$</from>
        <to type="forward">/$1/$2.YS?$3=$4&amp;$5=$6&amp;$7=$8</to>
    </rule>
    
</urlrewrite>


